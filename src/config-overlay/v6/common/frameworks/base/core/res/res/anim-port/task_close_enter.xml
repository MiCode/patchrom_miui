<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:shareInterpolator="false"
    android:zAdjustment="bottom" >
    <alpha android:interpolator="@interpolator/decelerate_cubic"
        android:duration="133"
        android:fillBefore="true"
        android:fillAfter="true"
        android:startOffset="600"
        android:fromAlpha="0.6"
        android:toAlpha="1.0"
        android:fillEnabled="true" />
    <translate android:interpolator="@interpolator/decelerate_cubic"
        android:duration="433" android:fillBefore="true"
        android:fillAfter="true"
        android:startOffset="300"
        android:fromYDelta="10%"
        android:toYDelta="0.0"
        android:fillEnabled="true" />
    <scale android:interpolator="@interpolator/linear"
        android:duration="433"
        android:pivotX="50.0%p"
        android:pivotY="0.0%p"
        android:fillBefore="true"
        android:fillAfter="true"
        android:startOffset="300"
        android:fromXScale="0.9"
        android:toXScale="1.0"
        android:fromYScale="0.9"
        android:toYScale="1.0"
        android:fillEnabled="true" />
</set>
