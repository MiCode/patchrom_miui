<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Backup format -->
    <string-array name="format_for_exported_note">
        <item>-%s</item>      <!-- format_folder_name -->
        <item>--%s</item>     <!-- format_folder_note_date -->
        <item>--%s</item>     <!-- format_note_date -->
        <item>--%s</item>     <!-- format_note_content -->
    </string-array>

    <string-array name="contact_entries">
        <item>Name</item>
        <item>Nickname</item>
        <item>Phone</item>
        <item>E-mail</item>
        <item>Organization</item>
        <item>IM</item>
        <item>SIP Phone</item>
        <item>Birthday</item>
        <item>Website</item>
        <item>Address</item>
        <item>Note</item>
    </string-array>

    <string-array translatable="false" name="contact_values">
        <item>vnd.android.cursor.item/name</item>
        <item>vnd.android.cursor.item/nickname</item>
        <item>vnd.android.cursor.item/phone_v2</item>
        <item>vnd.android.cursor.item/email_v2</item>
        <item>vnd.android.cursor.item/organization</item>
        <item>vnd.android.cursor.item/im</item>
        <item>vnd.android.cursor.item/sip_address</item>
        <item>vnd.android.cursor.item/contact_event</item>
        <item>vnd.android.cursor.item/website</item>
        <item>vnd.android.cursor.item/postal-address_v2</item>
        <item>vnd.android.cursor.item/note</item>
    </string-array>

    <string-array name="font_size_names">
        <item>Small</item>
        <item>Default</item>
        <item>Large</item>
        <item>Super</item>
    </string-array>

    <string-array name="count_entries">
        <item>0</item>
        <item>1</item>
        <item>2</item>
        <item>3</item>
        <item>4</item>
        <item>5</item>
        <item>6</item>
        <item>7</item>
        <item>8</item>
        <item>9</item>
    </string-array>

    <array name="count_values">
        <item>@drawable/folder_digit_0</item>
        <item>@drawable/folder_digit_1</item>
        <item>@drawable/folder_digit_2</item>
        <item>@drawable/folder_digit_3</item>
        <item>@drawable/folder_digit_4</item>
        <item>@drawable/folder_digit_5</item>
        <item>@drawable/folder_digit_6</item>
        <item>@drawable/folder_digit_7</item>
        <item>@drawable/folder_digit_8</item>
        <item>@drawable/folder_digit_9</item>
