<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">Key Chain</string>

    <!-- Dialog title when no certificates were found -->
    <string name="title_no_certs">No se encontraron certificados</string>

    <!-- Dialog title when at least one certificate was found -->
    <string name="title_select_cert">Elegir certificado</string>

    <!-- Used at top of dialog to identify requesting application -->
    <string name="requesting_application">La aplicación %s ha solicitado un certificado. Elegir un certificado garantizará a la aplicación la habilidad de usar esta identidad con los servidores ahora y en el futuro.</string>

    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->
    <string name="requesting_server">La aplicacion ha identificado la solicitud del servidor como %s pero sólo deberías garantizar el acceso de la aplicación al certificado si tu confias en ella.</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->
    <string name="install_new_cert_message">Puedes instalar certificados desde un archivo PKCS#12 con extensión %1$s o %2$s, desde el almacenamiento interno.</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate -->
    <string name="install_new_cert_button_label">Instalar</string>

    <!-- Label of button to send the allow the application certificate request -->
    <string name="allow_button">Permitir</string>

    <!-- Label of button to send the deny the application certificate request -->
    <string name="deny_button">Denegar</string>

</resources>
